{
  "name": "open-car",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:server": "cd server && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "next build",
    "build:server": "cd server && npm run build",
    "build:setup": "cd setup && npm run build",
    "build:all": "npm run build && npm run build:server && npm run build:setup",
    "start": "next start",
    "start:server": "cd server && npm run start",
    "start:all": "concurrently \"npm run start\" \"npm run start:server\"",
    "setup": "cd setup && npm run setup-all",
    "setup:can": "cd setup && npm run setup-can",
    "setup:system": "cd setup && npm run setup-system",
    "lint": "eslint --fix"
  },
  "dependencies": {
